<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#363333"
    tools:context="fr.wildcodeschool.eateam.knowledgeishere.Main2Activity">


    <Button
        android:text="Dashboard"
        android:textColor="@color/white"
        android:background="@color/transparent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="140dp"
        android:id="@+id/txtDasb"
        android:onClick="onClick"
        android:clickable="true"
        android:textSize="25sp"
        android:textAllCaps="false"
        android:fontFamily="roboto"
        android:textStyle="normal|bold" />

    <Button
        android:text="Action Plan"
        android:textColor="@color/white"
        android:background="@color/transparent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtDasb"
        android:layout_alignStart="@+id/txtDasb"
        android:layout_marginTop="10dp"
        android:onClick="onClick"
        android:id="@+id/txtAct"
        android:clickable="true"
        android:textSize="25sp"
        android:textAllCaps="false"
        android:fontFamily="roboto"
        android:textStyle="normal|bold" />

    <Button
        android:text="News Feed"
        android:textColor="@color/white"
        android:background="@color/transparent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtAct"
        android:layout_alignStart="@+id/txtAct"
        android:layout_marginTop="10dp"
        android:onClick="onClick"
        android:id="@+id/txtNews"
        android:clickable="true"
        android:textSize="25sp"
        android:textAllCaps="false"
        android:fontFamily="roboto"
        android:textStyle="normal|bold" />

    <Button
        android:text="..."
        android:textColor="@color/white"
        android:background="@color/transparent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:id="@+id/txtPoint"
        android:onClick="onClick"
        android:clickable="true"
        android:textSize="25sp"
        android:layout_below="@+id/txtSett"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:fontFamily="roboto"
        android:textStyle="normal|bold" />

    <Button
        android:text="Settings"
        android:textColor="@color/white"
        android:background="@color/transparent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:id="@+id/txtSett"
        android:onClick="onClick"
        android:clickable="true"
        android:textSize="25sp"
        android:layout_below="@+id/txtNews"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:fontFamily="roboto"
        android:textStyle="normal|bold" />

</RelativeLayout>
