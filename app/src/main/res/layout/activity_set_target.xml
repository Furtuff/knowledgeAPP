<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_set_target"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="fr.wildcodeschool.eateam.knowledgeishere.SetTargetActivity">

    <TextView
        android:text="Dimension C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/seekBar4"
        android:layout_alignParentStart="true"
        android:id="@+id/textView5" />

    <SeekBar
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="39dp"
        android:id="@+id/seekBar3"
        android:layout_below="@+id/seekBar2"
        android:layout_toEndOf="@+id/textView5"
        android:progress="4"
        android:max="10" />

    <SeekBar
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:id="@+id/seekBar1"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/seekBar3"
        android:max="10"
        android:progress="3"
        android:clickable="false" />

    <SeekBar
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="39dp"
        android:id="@+id/seekBar4"
        android:layout_below="@+id/seekBar3"
        android:layout_alignStart="@+id/seekBar3"
        android:max="10"
        android:progress="3" />

    <SeekBar
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:id="@+id/seekBar2"
        android:layout_below="@+id/seekBar1"
        android:layout_alignStart="@+id/seekBar1"
        android:max="10"
        android:progress="5" />

    <TextView
        android:text="Dimension A"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/seekBar1"
        android:layout_alignParentStart="true"
        android:id="@+id/textView6" />

    <TextView
        android:text="Dimension B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/seekBar2"
        android:layout_alignParentStart="true"
        android:id="@+id/textView7" />

    <TextView
        android:text="Dimension D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/seekBar4"
        android:layout_alignParentStart="true"
        android:id="@+id/textView8" />

    <TextView
        android:text="4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/seekBar4"
        android:layout_alignStart="@+id/textView10"
        android:id="@+id/textView11" />

    <TextView
        android:text="5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:layout_alignTop="@+id/seekBar2"
        android:layout_toEndOf="@+id/seekBar2" />

    <TextView
        android:text="3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:layout_alignTop="@+id/seekBar4"
        android:layout_toEndOf="@+id/seekBar4" />

    <TextView
        android:text="3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:layout_alignBottom="@+id/seekBar1"
        android:layout_toEndOf="@+id/seekBar1" />

</RelativeLayout>
